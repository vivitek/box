{
  "name": "vivirouter",
  "version": "0.0.1",
  "description": "Router for Vivi",
  "main": " ",
  "scripts": {
    "test": "jest --config jest.config.js --collectCoverage",
    "test:watch": "jest --config jest.config.js --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext ts",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.@(ts)": [
      "yarn format",
      "yarn lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awalshy/deep-thought.git"
  },
  "keywords": [
    "router",
    "dhcp",
    "vivi",
    "vivitek",
    "vincipit"
  ],
  "author": "Vincipit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awalshy/deep-thought/issues"
  },
  "homepage": "https://github.com/awalshy/deep-thought#readme",
  "dependencies": {
    "amqplib": "^0.6.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.3.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bluebird": "^3.5.33",
    "@types/node": "^14.10.0",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.2"
  }
}
