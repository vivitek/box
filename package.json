  "homepage": "https://github.com/awalshy/deep-thought#readme",
  "dependencies": {
    "amqplib": "^0.6.0",
    "lint-staged": "^10.3.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/node": "^14.10.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
