{
  "name": "vivirouter",
  "version": "0.0.1",
  "description": "Router for Vivi",
  "main": " ",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.ts\"",
